package com.oops;

public class Box {
	int length,breadth,height;
	
	Box(int length,int breadth,int height){
		this.length=length;
		this.breadth=breadth;
		this.height=height;
	}
	void display() {
		System.out.println("area and volume of cube");
		System.out.println(2*length*breadth+2*length*height+2*height*breadth);
	}

}
